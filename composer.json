{
    "name": "jolicode/automapper",
    "type": "library",
    "description": "JoliCode AutoMapper",
    "keywords": [],
    "homepage": "https://github.com/jolicode/automapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        },
        {
            "name": "Baptiste Leduc",
            "email": "baptiste.leduc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^4.18 || ^5.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0 || ^7.0",
        "symfony/expression-language": "^5.4 || ^6.0 || ^7.0",
        "symfony/deprecation-contracts": "^2.0|^3.0",
        "symfony/property-info": "^5.4.23 || ^6.2.10 || ^7.0"
    },
    "require-dev": {
        "doctrine/annotations": "~1.0",
        "moneyphp/money": "^3.3.2",
        "phpdocumentor/type-resolver": "^1.7",
        "phpunit/phpunit": "^9.0",
        "symfony/filesystem": "^5.4 || ^6.0 || ^7.0",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/serializer": "*",
        "symfony/uid": "^5.4 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
    },
    "suggest": {
        "symfony/serializer": "Allow to use symfony serializer attributes in mapping"
    },
    "conflict": {
        "symfony/dependency-injection": "<5.4",
        "symfony/framework-bundle": "<5.4",
        "symfony/serializer": "<5.4"
    },
    "autoload": {
        "psr-4": {
            "AutoMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutoMapper\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Bundle/Resources/App/AppKernel.php"
        ]
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
